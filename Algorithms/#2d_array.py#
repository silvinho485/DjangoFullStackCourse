arr = [[x for x in range(6)] for j in range(4)]
arr2 = [x for x in range(12)]
arr3 = []
arr4 = []
lines = 5
columns = 6


for i in range(3):
    arraux = []
    for x in range(5):
        arraux.append(x)
    arr3.append(arraux)

def matMaker(line, column, mat):
    for liner in range(line):
        auxiliarArray = []
        for columner in range(column):
            auxiliarArray.append(columner)
        mat.append(auxiliarArray)
    return mat

print(matMaker(lines, columns, arr4))

